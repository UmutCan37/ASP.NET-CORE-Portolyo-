@model Feature
@{
    ViewData["Title"] = "UpdateFeature";
    Layout = "~/Views/Layout/Index.cshtml";
}

div class="content">
<div class="container-fluid">
	<h4 class="page-title">Deneyimler</h4>
	<div class="row">
		<div class="col-md-12">
			<div class="card">
				<div class="card-header">
					<div class="card-title">Deneyim Ekleme Sayfası</div>
				</div>
				<div class="card-body">
					<form method="post">
						<input type="hidden" asp-for="FeatureId" />
						<label>Başlık</label>
						<input type="text" asp-for="Title" class="form-control" />
						<br />
						<label>Açıklama</label>
						<input type="text" asp-for="Description" class="form-control" />
						<br />

						<button class="btn btn-primary">Öne Çıkanlar Güncelle</button>
					</form>

				</div>
			</div>
		</div>
	</div>
</div>
</div>


